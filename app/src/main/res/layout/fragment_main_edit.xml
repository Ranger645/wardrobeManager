<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/light_background_round_rect_bg"
    tools:context=".ui.add_item.MainEditFragment">

    <com.android.wardrobeManager.ui.add_item.SquareFrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/rounded_panel_margin_size_double"
            android:scrollbars="none">

            <LinearLayout
                android:id="@+id/additem_edit_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            </LinearLayout>
        </ScrollView>
    </com.android.wardrobeManager.ui.add_item.SquareFrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>